{
  "url": "https://decora.ee",
  "category": {
    "parent-selector": ".categories-widget",
    "selector": ".category-link",
    "attribute": "href"
  },
  "popular-category":{
    "parent-tag": "div",
    "parent-selector": "data-content-type",
    "selector": "a",
    "attribute": "href"
  },
  "product": {
    "item": {
      "selector": ".product-item-info"
    },
    "image": {
      "selector": ".product-image-photo",
      "attribute": "src"
    },
    "title": {
      "selector": ".product-item-link",
      "attribute": "title"
    },
    "url": {
      "selector": ".product-item-link",
      "attribute": "href"
    },
    "price": {
      "selector": ".price",
      "attribute": "text"
    }
  }
}
